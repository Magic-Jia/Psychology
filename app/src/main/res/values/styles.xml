        <style name="bulletImageStyle">
            <item name="cornerFamilyTopLeft">rounded</item>
            <item name="cornerFamilyTopRight">rounded</item>
             <item name="cornerSizeTopLeft">6dp</item>
             <item name="cornerSizeTopRight">6dp</item>
        </style>
</resources>